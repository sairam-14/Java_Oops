package com.inheritance;

public class Hirarchial_Animal {

	int legs = 4;

	public static void main(String[] args) {
		Hirarchial_Animal h = new Hirarchial_Animal();
		h.waik();
	}

	public void waik() {
		System.out.println("Waiking with 4 legs");
	}

}

class tiger extends Hirarchial_Animal {

	String color = "golden brown";
	String food = "Nonveg";

}

class cat extends Hirarchial_Animal {

	String clour = "white";
	String food = "veg non veg";
}

class zebra extends Hirarchial_Animal {

	String color = "Black and white";
	String food = "veg";
}
